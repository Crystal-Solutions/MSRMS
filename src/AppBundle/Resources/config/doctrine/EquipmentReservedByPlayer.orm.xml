<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\EquipmentReservedByPlayer" table="equipment_reserved_by_player">
    <indexes>
      <index name="fk_equipment_has_player_player1_idx" columns="player_id"/>
      <index name="fk_equipment_has_player_equipment1_idx" columns="equipment_id"/>
      <index name="fk_equipment_reserved_by_player_authorizing_officer1_idx" columns="authorizing_officer_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="start" type="datetime" column="start" nullable="true"/>
    <field name="end" type="datetime" column="end" nullable="true"/>
    <field name="amount" type="integer" column="amount" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="authorizingOfficer" target-entity="AuthorizingOfficer" fetch="LAZY">
      <join-columns>
        <join-column name="authorizing_officer_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="player" target-entity="Player" fetch="LAZY">
      <join-columns>
        <join-column name="player_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="equipment" target-entity="Equipment" fetch="LAZY">
      <join-columns>
        <join-column name="equipment_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
