<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TimeSlotEquipment" table="time_slot_equipment">
    <indexes>
      <index name="fk_time_slot_equipment_sport_has_equipment1_idx" columns="sport_has_equipment_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="startTime" type="time" column="start_time" nullable="true"/>
    <field name="endTime" type="time" column="end_time" nullable="true"/>
    <field name="day" type="string" column="day" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="sportHasEquipment" target-entity="SportHasEquipment" fetch="LAZY">
      <join-columns>
        <join-column name="sport_has_equipment_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
