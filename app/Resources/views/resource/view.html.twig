{# app/Resources/views/resource/view.html.twig #}
{% extends 'base.html.twig' %}

{% block title %}
Resource Details
{% endblock %}

{% block body %}

<table width="100%">
	<tr>
		<th>Name</th>
		<th>Description</th>
		<th>Instructor Name</th>
		<th>Location</th>
	</tr>
<ul>

	<tr>
	<td>{{ resource.name|e }}
	</td>

	<td>{{ resource.description|e }}
	</td>
	<td>{{ resource.instructorName|e }}
	</td>

	<td>{{ resource.location|e }}
	</td>
	</tr>


</ul>
{% endblock %}

